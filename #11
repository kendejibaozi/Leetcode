class Solution {
    public int maxArea(int[] height) {
        
        int n = height.length;
        int left = 0, right = n - 1, res = 0;
        while (left < right) {
            res = Math.max(res, Math.min(height[left], height[right]) * (right - left));
            if (height[left] <= height[right]) left++;
            else right--;
        }
        return res;

    }
}
